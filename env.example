# Basic environment template for Mega Jarvis
# Copy to .env and edit values before running docker compose.

# General
TZ=UTC
DOMAIN=example.local

# Traefik / Proxy
TRAEFIK_ENABLED=true

# n8n
N8N_BASIC_AUTH_ACTIVE=true
N8N_USER=n8n
N8N_PASSWORD=changeme

# AI assistant placeholders
AI_MODEL_PATH=./tools/ai_assistant/models

# SMTP (email) placeholder
SMTP_HOST=smtp.example.local
SMTP_PORT=1025
SMTP_USER=
SMTP_PASSWORD=

# Database placeholders (if needed)
POSTGRES_USER=jarvis
POSTGRES_PASSWORD=changeme
POSTGRES_DB=jarvis_db
